# Development Environment Configuration
# Optimized for debugging and development

# Server Configuration
PORT=8069
HOSTNAME=127.0.0.1
NODE_ENV=development

# Performance Settings (Lower for development)
MAX_STREAMS=50
MAX_BUFFER_SIZE=1048576
REQUEST_TIMEOUT=300000
KEEP_ALIVE_TIMEOUT=65000

# Development Logging Configuration (Optimized)
LOG_LEVEL=debug
LOG_COLORS=true
LOG_TIMESTAMPS=false
LOG_CATEGORIES=true
CHUNK_LOG_FREQUENCY=0
ENABLE_PROGRESS_LOGS=true
ENABLE_ENDPOINT_LOGS=true
ENABLE_MODEL_LOGS=true
ENABLE_MEMORY_LOGS=true

# Monitoring (More frequent for development)
METRICS_ENABLED=true
PERFORMANCE_METRICS=true
MEMORY_CHECK_INTERVAL=15000

# Security Settings (Relaxed for development)
ENABLE_CORS=true
ALLOWED_ORIGINS=*
ENABLE_RATE_LIMIT=false
MAX_REQUESTS_PER_MINUTE=1000

# Performance Optimization (Disabled for development)
ENABLE_GC=false
MEMORY_THRESHOLD_MB=500
ENABLE_COMPRESSION=false
CACHE_HEADERS=false
