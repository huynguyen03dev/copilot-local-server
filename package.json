{
  "name": "vscode-copilot-api-server",
  "version": "1.0.0",
  "description": "Local API server that exposes GitHub Copilot as OpenAI-compatible endpoint",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "start:prod": "NODE_ENV=production bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "type-check": "tsc --noEmit",
    "test": "bun run examples/test-client.ts",
    "test-streaming": "python test.py",
    "test-errors": "bun test tests/streaming-error.test.ts",
    "test-integration": "bun test tests/integration.test.ts",
    "test-performance": "python performance-test.py",
    "test-all": "bun run test-streaming && bun run test-errors && bun run test-integration && bun run test-performance",
    "test-endpoint": "bun run test-endpoint.ts",
    "test-logging": "bun run test-logging.ts",
    "test-stop": "python test-stop-param.py",
    "debug": "python debug-test.py",
    "monitor": "python monitoring-dashboard.py",
    "monitor-perf": "python monitor-streaming.py",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "auth": "bun run src/index.ts --auth",
    "clear-auth": "bun run src/index.ts --clear-auth"
  },
  "dependencies": {
    "hono": "^4.6.3",
    "zod": "^3.23.8",
    "@hono/zod-validator": "^0.2.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "github-copilot",
    "openai-api",
    "local-server",
    "vscode"
  ],
  "author": "",
  "license": "MIT"
}
